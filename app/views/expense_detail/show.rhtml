<table class="show_table">
<%- for column in ExpenseDetail.columns -%>
  <%- if system_column?(column.name) then next end -%>
<tr>
  <th><%= getLongName('expense_details', column.name) %></th><td><%=h @expense_detail.send(column.name) %></td>
</tr>
<% end %>
</table>
<%= link_to '編集', :action => 'edit', :id => @expense_detail %> |
<%= link_to '一覧', :action => 'list' %>

<h1><%=getLongName('table_name','bp_members')%>新規作成</h1>

<%= form_tag :action => 'create' do %>
  <%= render :partial => 'form' %>
  <div class="submit_box">
    <%= submit_tag '登録', :class => "btn btn-primary btn-medium" %>
    <% if params[:import_mail_id] && params[:template_id] %>
      <%= link_or_back '解析テンプレート選択に戻る', {:controller => 'analysis_template', :action => 'list'} %>
    <% else %>
      <%= link_or_back '戻る', {:action => 'list',:popup => params[:popup]} %>
    <% end %>
  </div>
  <%= popup_hidden_field %>
<% end %>


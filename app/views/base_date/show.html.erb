<table class="show_table">
<%- for column in BaseDate.columns -%>
  <%- if system_column?(column.name) then next end -%>
<tr>
  <th><%= getLongName('base_dates', column.name) %></th><td><%=h @base_date.send(column.name) %></td>
</tr>
<% end %>
</table>
